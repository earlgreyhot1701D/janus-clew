# Janus Clew Configuration
# Copy this to .env and fill in your values
# NEVER commit .env to git

# ============================================================================
# AWS CONFIGURATION
# ============================================================================

# AWS region for Bedrock/AgentCore
AWS_REGION=us-east-1

# AWS Account ID (required for AgentCore integration)
AWS_ACCOUNT_ID=your-account-id

# ============================================================================
# JANUS CONFIGURATION
# ============================================================================

# Environment (development, production, testing)
JANUS_ENV=development

# Verbose logging
JANUS_VERBOSE=false

# Enable caching for analyses
JANUS_CACHE=true

# ============================================================================
# SERVER CONFIGURATION
# ============================================================================

# API server host and port
API_HOST=127.0.0.1
API_PORT=3001

# Frontend dev server
FRONTEND_HOST=127.0.0.1
FRONTEND_PORT=5173

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3001

# ============================================================================
# LOGGING
# ============================================================================

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=DEBUG

# ============================================================================
# ADVANCED (Usually don't change these)
# ============================================================================

# Data directory (stores analyses)
# JANUS_DATA_DIR=~/.janus-clew
